{
	"name": "sveltekit-starter1",
	"private": true,
	"author": "Raj",
	"type": "module",
	"version": "0.0.1",
	"license": "UNLICENSED",
	"packageManager": "pnpm@9.4.0",
	"description": "sveltekit-starter collection site 1",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">= 9.0.0",
		"npm": "Please use pnpm instead of npm.",
		"yarn": "Please use pnpm instead of yarn."
	},
	"bugs": {
		"email": "root@rajeshpandey.site",
		"url": "https://github.com/RajeshPandey057/sveltekit-starter1/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@bitbucket.org/RajeshPandey057/sveltekit-starter1.git"
	},
	"scripts": {
		"dev": "vite dev --host --port 3000",
		"build": "vite build --mode ci",
		"preview": "vite preview",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"db:push": "drizzle-kit push",
		"format": "prettier . --check",
		"format:fix": "prettier . --write",
		"db:studio": "drizzle-kit studio",
		"db:migrate": "drizzle-kit migrate",
		"db:generate": "drizzle-kit generate",
		"prepare": "node .husky/install.mjs",
		"test:unit": "vitest",
		"test:integration": "playwright test",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"email": "email dev --dir src/components/email",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"icon:generate": "pnpm dlx @vite-pwa/assets-generator --preset minimal-2023 static/favicon.svg"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
